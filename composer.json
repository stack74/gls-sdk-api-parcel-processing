{
  "name": "stack74/sdk-api-parcel-processing",
  "description": "GLS Web API for Parcel Processing SDK",
  "license": "MIT",
  "keywords": [
    "php",
    "sdk",
    "GLS"
  ],
  "authors": [
    {
      "email": "plugins@netresearch.de",
      "name": "Netresearch DTT GmbH",
      "role": "Manufacturer",
      "homepage": "https://www.netresearch.de/"
    }
